function bubblesort_list(L) {
 const len = length(L);
 for (let i = len - 1; i >= 1; i = i - 1) {
    const lst = L;    
    for (let j = 0 ; j< i; j= j+1){
        if (head(lst)> head(tail(lst))){
            const temp = head(lst); 
            set_head(lst, head(tail(lst)));
            set_head(tail(lst), temp);
            lst = tail(lst);
            
        }
    }
}
}

const LL = list(3, 5, 2, 4, 1);
bubblesort_list(LL);
LL;
    