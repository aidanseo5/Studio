//1 

function my_map(f, xs) {
  return accumulate((x,y)=>pair(f(x),y),null, xs); 
}
//my_map(x => x + 1, list(1, 2, 3));


function remove_duplicates(lst) {
 const org = lst ; 
 function iter(a, org ){
     return a !== head(org)
            ? iter(a,tail(org))
            : false;
 }
 return filter(x => iter(x,lst),lst );
}
remove_duplicates(list(1, 2, 3, 4, 4, 3, 2, 1, 2));